{
    "name": "ngmy/webloyer",
    "description": "Webloyer is a Web UI for managing Deployer deployments",
    "keywords": ["deployer", "deploy", "deployment"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuta Nagamiya",
            "email": "y.nagamiya@gmail.com"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ajaxorg/ace-builds",
                "version": "1.2.0",
                "source": {
                    "url": "git@github.com:ajaxorg/ace-builds.git",
                    "type": "git",
                    "reference": "v1.2.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "lou/multi-select",
                "version": "0.9.12",
                "source": {
                    "url": "git@github.com:lou/multi-select.git",
                    "type": "git",
                    "reference": "0.9.12"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fguillot/json-rpc",
                "version": "v1.2.6",
                "source" : {
                    "url": "git@github.com:matasarei/JsonRPC.git",
                    "type": "git",
                    "reference": "v1.2.6"
                }
            }
        }
    ],
    "require": {
        "php": "^7.1.0",
        "ajaxorg/ace-builds": "1.2.0",
        "davejamesmiller/laravel-breadcrumbs": "4.x",
        "deployer/deployer": "^4.0",
        "fideloper/proxy": "~3.3",
        "fguillot/json-rpc": "v1.2.6",
        "kodeine/laravel-acl": "~1.0@dev",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.5.*",
        "lou/multi-select": "0.9.12",
        "ngmy/eloquent-serialized-lob": "^0.1.0",
        "robclancy/presenter": "1.4.*",
        "sensiolabs/ansi-to-html": "~1.0",
        "symfony/yaml": "~3.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mikey179/vfsStream": "~1.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "php-coveralls/php-coveralls": "^2.1",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Ngmy\\Webloyer\\Webloyer\\": "packages/webloyer/src/",
            "Ngmy\\Webloyer\\IdentityAccess\\": "packages/identity_access/src/",
            "Ngmy\\Webloyer\\Common\\": "packages/common/src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/_helpers"
        ],
        "psr-4": {
            "Ngmy\\Webloyer\\Webloyer\\": "packages/webloyer/tests/",
            "Ngmy\\Webloyer\\IdentityAccess\\": "packages/identity_access/tests/",
            "Ngmy\\Webloyer\\Common\\": "packages/common/tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "@distributeAjaxorgAce",
            "@distributeLouMultiSelect"
        ],
        "post-update-cmd": [
            "@distributeAjaxorgAce",
            "@distributeLouMultiSelect"
        ],
        "distributeAjaxorgAce": [
            "@php -r \"file_exists('public/vendor/ajaxorg') || mkdir('public/vendor/ajaxorg', 0755, true);\"",
            "@php -r \"!is_link('public/vendor/ajaxorg/ace') || unlink('public/vendor/ajaxorg/ace');\"",
            "@php -r \"symlink(getcwd() . '/vendor/ajaxorg/ace-builds/src-min-noconflict', 'public/vendor/ajaxorg/ace');\""
        ],
        "distributeLouMultiSelect": [
            "@php -r \"file_exists('public/vendor/lou') || mkdir('public/vendor/lou', 0755, true);\"",
            "@php -r \"!is_link('public/vendor/lou/multi-select') || unlink('public/vendor/lou/multi-select');\"",
            "@php -r \"file_exists('public/vendor/lou/multi-select') || symlink(getcwd() . '/vendor/lou/multi-select', 'public/vendor/lou/multi-select');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.0"
        }
    }
}
